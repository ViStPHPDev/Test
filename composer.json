        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/skeleton": "dev-master",
        "symfony/dotenv": "^4.2@dev",
        "symfony/browser-kit": "^4.2@dev"
    },
    "autoload": {
        "psr-4": {
